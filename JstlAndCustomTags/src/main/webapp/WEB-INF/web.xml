<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>JstlAndCustomTags</display-name>
  <servlet>
        <servlet-name>InventoryServlet</servlet-name>
        <servlet-class>com.vamsi.jstlcustomtags.controller.InventoryServlet</servlet-class>
    </servlet>

    <!-- Servlet Mapping -->
    <servlet-mapping>
        <servlet-name>InventoryServlet</servlet-name>
        <url-pattern>/productList</url-pattern>
    </servlet-mapping>

    <!-- Welcome file -->
    <welcome-file-list>
        <welcome-file>inventory.jsp</welcome-file>
    </welcome-file-list>
</web-app>